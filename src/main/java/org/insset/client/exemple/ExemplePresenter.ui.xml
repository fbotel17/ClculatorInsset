<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:menu="urn:import:org.insset.client.menu">
    <g:HTMLPanel>
        <g:Label text="Nombre 1:" />
        <g:TextBox ui:field="nombre1" /> <!-- Renommé en nombre1 -->
        
        <g:Label text="Nombre 2:" />
        <g:TextBox ui:field="nombre2" /> <!-- Renommé en nombre2 -->

        <g:ResetButton ui:field="boutonClear" text="Clear" />
        <g:SubmitButton ui:field="boutonEnvoyer" text="Diviser" />
        
        <g:Label ui:field="errorLabel" />
    </g:HTMLPanel>
</ui:UiBinder>
